    "bitbake-setup": {
        "configurations": [
            {
                "name": "homeassistant",
                "description": "Homeassistant base configuration",
                "bb-layers": [
                    "openembedded-core/meta",
                    "meta-yocto/meta-poky",
                    "meta-openembedded/meta-oe",
                    "meta-openembedded/meta-python",
                    "meta-openembedded/meta-multimedia",
                    "meta-openembedded/meta-networking",
                    "meta-openembedded/meta-filesystems",
                    "meta-python-ai",
                    "meta-clang-revival",
                    "meta-sca",
                    "meta-virtualization",
                    "meta-rauc",
                    "meta-homeassistant"
                ],
                "bb-layers-file-relative": [
                    "meta-application"
                ],
                "oe-fragments": ["core/yocto/sstate-mirror-cdn", "application/ha"],
                "oe-fragments-one-of": {
                    "distro": {
                        "description": "Distribution variants",
                        "options": ["distro/poky-altcfg"]
                    }
                },
                "configurations": [
                    {
                        "name": "rpi",
                        "description": "Raspberry Pi 64",
                        "bb-layers": [
                            "meta-rauc-community/meta-rauc-raspberrypi",
                            "meta-raspberrypi"
                        ],
                        "oe-fragments": ["machine/raspberrypi-armv8"],
                        "setup-dir-name": "raspberrypi-armv8",
                        "use-full-setup-dir-name": false
                    },
                    {
                        "name": "qemu",
                        "description": "QEMU x86",
                        "bb-layers": [
                            "meta-rauc-community/meta-rauc-qemux86"
                        ],
                        "oe-fragments": ["machine/qemux86-64"],
                        "setup-dir-name": "qemux86-64",
                        "use-full-setup-dir-name": false
                    }
                ],
                "setup-dir-name": "setup",
                "use-full-setup-dir-name": false
            }
        ]
    }
}
